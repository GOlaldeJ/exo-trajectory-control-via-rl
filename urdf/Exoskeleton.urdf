      <origin xyz="0 0 -0.2"/>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.2"/>
      <mass value="7.0"/>
      <inertia ixx="0.097708" iyy="0.097708" izz="0.00875" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="left_shin">
    <visual>
      <origin xyz="0 0 -0.2"/>
      <geometry>
        <cylinder length="0.4" radius="0.04"/>
      </geometry>
      <material name="red2">
        <color rgba="0.5 0.2 0.2 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.2"/>
      <geometry>
        <cylinder length="0.4" radius="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.2"/>
      <mass value="4.0"/>
      <inertia ixx="0.054933" iyy="0.054933" izz="0.0032" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="left_ankle_pitch">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
