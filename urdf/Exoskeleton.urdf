<robot name="lower_limb_exoskeleton">

  <link name="upper_torso">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.8" radius="0.15"/>
      </geometry>
      <material name="blue">
        <color rgba="0.2 0.2 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.8" radius="0.15"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="50.0"/>
      <inertia ixx="2.947917" iyy="2.947917" izz="0.5625" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="lower_torso">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.6 0.1"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.6 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0308" iyy="0.0017" izz="0.0308" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="upper_torso_joint" type="fixed">
    <parent link="upper_torso"/>
    <child link="lower_torso"/>
    <origin xyz="0 0 -0.4" rpy="0 0 0"/>
  </joint>

  <!-- ========================= -->
  <!-- Right Leg -->
  <!-- ========================= -->

  <link name="right_hip_yaw">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
      <material name="gray2">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.0028" iyy="0.0028" izz="0.0028" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="right_hip_roll">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.00035" iyy="0.00035" izz="0.00035" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="right_thigh">
    <visual>
      <origin xyz="0 0 -0.2" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
      <material name="green1">
        <color rgba="0.1 0.5 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.2" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.2"/>
      <mass value="7.0"/>
      <inertia ixx="0.097708" iyy="0.097708" izz="0.00875" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="right_shin">
    <visual>
      <origin xyz="0 0 -0.2" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.04"/>
      </geometry>
      <material name="green2">
        <color rgba="0.2 0.5 0.2 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.2" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.2"/>
      <mass value="4.0"/>
      <inertia ixx="0.054933" iyy="0.054933" izz="0.0032" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="right_ankle_pitch">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
      <material name="gray2">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.0005" iyy="0.0005" izz="0.0005" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="right_foot">
    <contact>
        <lateral_friction value="1"/>
    </contact>
    <visual>
      <origin xyz="0.1 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.3 0.15 0.05"/>
      </geometry>
      <material name="green3">
        <color rgba="0.3 0.5 0.3 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.15 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.3 0.15 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.1 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.0016" iyy="0.0094" izz="0.0104" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="right_hip_yaw_joint" type="revolute">
    <parent link="lower_torso"/>
    <child link="right_hip_yaw"/>
    <origin xyz="0 -0.25 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.75" upper="0.75" effort="100" velocity="5.2"/>
  </joint>

  <joint name="right_hip_roll_joint" type="revolute">
    <parent link="right_hip_yaw"/>
    <child link="right_hip_roll"/>
    <origin xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.75" upper="0.5" effort="150" velocity="5.2"/>
  </joint>

  <joint name="right_hip_pitch_joint" type="revolute">
    <parent link="right_hip_roll"/>
    <child link="right_thigh"/>
    <origin xyz="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.25" upper="0.4" effort="300" velocity="8.7"/>
  </joint>

  <joint name="right_knee_joint" type="revolute">
    <parent link="right_thigh"/>
    <child link="right_shin"/>
    <origin xyz="0 0 -0.4"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.09" upper="2.44" effort="350" velocity="7.9"/>
  </joint>

  <joint name="right_ankle_roll_joint" type="revolute">
    <parent link="right_shin"/>
    <child link="right_ankle_pitch"/>
    <origin xyz="0 0 -0.4"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.5" upper="0.5" effort="180" velocity="6.1"/>
  </joint>

  <joint name="right_ankle_pitch_joint" type="revolute">
    <parent link="right_ankle_pitch"/>
    <child link="right_foot"/>
    <origin xyz="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.5" upper="0.5" effort="100" velocity="3.5"/>
  </joint>

  <!-- ========================= -->
  <!-- Left Leg (mirroring the right leg) -->
  <!-- ========================= -->

  <link name="left_hip_yaw">
    <visual>
      <origin xyz="0 0 0"/>
