      <inertia ixx="0.0005" iyy="0.0005" izz="0.0005" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="left_foot">
    <contact>
        <lateral_friction value="1"/>
    </contact>
    <visual>
      <origin xyz="0.1 0 0"/>
      <geometry>
        <box size="0.3 0.15 0.05"/>
      </geometry>
      <material name="red3">
        <color rgba="0.5 0.3 0.3 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.1 0 0"/>
      <geometry>
        <box size="0.3 0.15 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.15 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.0016" iyy="0.0094" izz="0.0104" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="left_hip_yaw_joint" type="revolute">
    <parent link="lower_torso"/>
    <child link="left_hip_yaw"/>
    <origin xyz="0 0.25 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.75" upper="0.75" effort="100" velocity="5.2"/>
  </joint>

  <joint name="left_hip_roll_joint" type="revolute">
    <parent link="left_hip_yaw"/>
    <child link="left_hip_roll"/>
    <origin xyz="0 0 0"/>
